{% if response.connection == "WEBSOCKET"  %}


<h3>Connection </h3>
<div class="wrapper" >
	<div class="table-responsive" >
		<table class="table table-striped table-bordered">
			<thead>
				<tr>
					<th>KEY</th>
					<th>VALUE</th>
				</tr>
			</thead>
			<tbody>
			{% for key, ele in response %}
				{% if key != "config"  %}
					{% if key == "message"  %}
						<tr>
							<td>{{key}}</td>
							<td>{{ele|length}}</td>
						</tr>
					{% else %}
						<tr>
							<td>{{key}}</td>
							<td>{{ele}}</td>
						</tr>
					{% endif %}
				{% endif %}
			{% endfor %}
			</tbody>
		</table>
	</div>
</div>


<h3>CONNECTION WEBSOCKET CONFIG</h3>

	<div class="wrapper" >
	<div class="table-responsive" >
		<table class="table table-striped table-bordered">
			<thead>
				<tr>
					<th>KEY</th>
					<th>VALUE</th>
				</tr>
			</thead>
			<tbody>
			{% for key, ele in response.config %}
			<tr>
				<td>{{key}}</td>
				<td>{{ele}}</td>
			</tr>
			{% endfor %}
			</tbody>
		</table>
	</div>
</div>


{% else %}

<h3>Response </h3>
<div class="wrapper" >
	<div class="table-responsive" >
		<table class="table table-striped table-bordered">
			<thead>
				<tr>
					<th>KEY</th>
					<th>VALUE</th>
				</tr>
			</thead>
			<tbody>
			{% for key, ele in response %}
			<tr>
				<td>{{key}}</td>
				<td>{{ele}}</td>
			</tr>
			{% endfor %}
			</tbody>
		</table>
	</div>
</div>

{% endif %}




{% extends "./layout.json.twig" %}

{% block body %}
"server":"nodefony",
"code":404,
{% if error.errorType %}
"type":"{{error.errorType}}",
{% else %}
"type":"Error",
{% endif %}
{% if error.pdu %}
"pdu":{{ error.pdu }},
{% endif %}
"url":"{{error.url }}",
{% if error.message %}
"message":"{{error.message }}"
{% else %}
"message":"Not Found"
{% endif %}
{% endblock %}

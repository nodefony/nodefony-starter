<!DOCTYPE html>
<html lang="{{nodefony.local|escape}}">
  <head>
    {% block head %}{% endblock %}
    {% block meta %}
      <meta charset="utf-8">
      <meta http-equiv="content-type" content="text/html; charset=UTF-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge">
      <meta name="viewport" content="width=device-width, initial-scale=1">
      <meta name="generator" content="Nodefony (https://nodefony.net)">
      <meta name="keywords" content="web,Framework,node.js,javascript,npm,linux">
      <link rel="icon" type="image/x-icon" href="/app/favicon.ico"/>
    {% endblock %}
    {% block description %}
      <meta name="description" content="Nodefony Node.js full-stack web framework">
    {% endblock %}
    <title>
      {% block title %}{{nodefony.name|escape}}{% endblock %}
    </title>
    {% block stylesheets %}
      <!-- WEBPACK BUNDLE -->
      <link rel="stylesheet" href="{{CDN('stylesheet')}}/app/assets/css/app.css"/>
{% endblock %}
  </head>
  <body>
    {% block body %}{% endblock %}
    {% block javascripts %}
      <!-- WEBPACK BUNDLE -->
      <script type="text/javascript" src="{{CDN('javascript')}}/app/assets/js/app.js"></script>
{% endblock %}
  </body>
</html>
